

<div class="container">
  <div class="row">
<div class="col-xs-3">
<%= render 'users/list', user: @user %>
<%= render 'books/list', book:@book %>
</div>

<div class="col-xs-9">
  <h1>Users</h1>
<table>
  <thead>
    <tr>
      <th>image</th>
      <th>name</th>
      <th colspan="1"></th>
    </tr>
  </thead>
  <tbody>
 <% @users.each do |user| %>
 <tr>
   <td><%= attachment_image_tag user, :profile_image, :fill, 80, 80 , fallback: "no_image.jpg" %></td>
  <td><%= user.name %></td>
  <td><%= link_to "Show", user_path(user) %></td>
  </tr>
 <%end%>
<tbody>
</table>
</div>
</div>
</div>
